using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace arrays5
{
    class Program
    {
        static void Main(string[] args)
        {
            /*diccionarios*/

            var diccionario = new Dictionary<int, string>
            {
                {1,"lunes"},
                {2,"martes"},
                {3,"martes"},
                {4,"miercoles"},
                {5,"jueves"},
                {6,"viernes"},
                {7,"sabado"},
                {8,"domingo"}
            };
            Console.WriteLine("indica un numeros entre 1 y 8 para saber el dia de cada num.: ");
            int num = int.Parse(Console.ReadLine());
            Console.WriteLine($"{num} es el dia {diccionario[num]}");

            Console.ReadKey();

        }
    }
}
