using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace a単oBiciesto
{
    class Program
    {
        static void Main(string[] args)
        {
            int year;
            Console.WriteLine("Ingrese un a単o : ");
            year = int.Parse(Console.ReadLine());

            if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0)
            {

                Console.WriteLine($"El a単o {year} es BICIESTO");
            }

            else
            {
                Console.WriteLine($"El a単o {year} NO es biciesto");
            }


            Console.ReadKey();


        }
    }
}
